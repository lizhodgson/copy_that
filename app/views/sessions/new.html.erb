

<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %> 
  </div>  

  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %> 
  </div>  

  <div class="action">
    <%= submit_tag "Submit the form that logs you in" %>
  </div> 
<% end %>